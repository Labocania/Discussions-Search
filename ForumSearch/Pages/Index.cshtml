@page
@{
    ViewData["Title"] = "Main";
}
<form asp-page="./Index" method="get">
    <nav aria-label="Search">
        <div class="form-actions no-color row" id="primary-form">
            <div class="col-10"><input type="text" name="q" placeholder="Select forums on the list and search the Web here." class="form-control" id="primary-field" /></div>
            <div class="col-2"><input type="submit" value="Search" class="btn btn-primary" id="primary-search" /></div>
        </div>
    </nav>
    <div id="search-query" class="gcse-searchresults-only" data-mobileLayout="enabled"></div>
</form>





